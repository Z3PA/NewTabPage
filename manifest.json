{
  "manifest_version": 3,
  "name": "Z3PA New Tab",
  "version": "2.0.0",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "icons/favicon.png",
        "services.html",
        "services.js",
        "links.html",
        "links.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "icons": {
    "16": "icons/favicon.png",
    "32": "icons/favicon.png",
    "48": "icons/favicon.png",
    "128": "icons/favicon.png"
  }
}